
<h1>Students</h1>
 
<table>
  <tr>
    <th>First Name:</th>
    <th>Last Name:</th>
    <th></th>
  </tr>
 
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= link_to 'SchoolClasses', student_path(student) %></td>
    </tr>
  <% end %>
</table>